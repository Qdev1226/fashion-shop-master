  body.animsition
    // Header
    header.header1
      // Header desktop
      .container-menu-header
        .topbar
          .topbar-social
            a.topbar-social-item.fa.fa-facebook(href='https://github.com/cuongw')
            a.topbar-social-item.fa.fa-instagram(href='https://github.com/cuongw')
            a.topbar-social-item.fa.fa-pinterest-p(href='https://github.com/cuongw')
            a.topbar-social-item.fa.fa-snapchat-ghost(href='https://github.com/cuongw')
            a.topbar-social-item.fa.fa-youtube-play(href='https://github.com/cuongw')
          span.topbar-child1
            | Free shipping for standard order over $100
          .topbar-child2
            span.topbar-email
              | info@fashionshop.com
            .topbar-language.rs1-select2
              select.selection-1(name='time')
                option USD
                option EUR
        .wrap_header
          // Logo
          a.logo(href='/')
            img(src='images/icons/logo.png', alt='IMG-LOGO')
          // Menu
          .wrap_menu
            nav.menu
              ul.main_menu
                li
                  a(href='/') Home
                li
                  a(href='/products') Shop
                li.sale-noti
                  a(href='/products') Sale
                li
                  a(href='/cart') Features
                li
                  a(href='/blog') Blog
                li
                  a(href='/about') About
                li
                  a(href='/contact') Contact
          // Header Icon
          .header-icons
            a.header-wrapicon1.dis-block(href='#')
              img.header-icon1(src='images/icons/icon-header-01.png', alt='ICON')
            span.linedivide1
            a.header-wrapicon2(href='/cart')
              img.header-icon1.js-show-header-dropdown(src='images/icons/icon-header-02.png', alt='ICON')
              span#quantity.header-icons-noti 0
      // Header Mobile
      .wrap_header_mobile
        // Logo moblie
        a.logo-mobile(href='/')
          img(src='images/icons/logo.png', alt='IMG-LOGO')
        // Button show menu
        .btn-show-menu
          // Header Icon mobile
          .header-icons-mobile
            a.header-wrapicon1.dis-block(href='#')
              img.header-icon1(src='images/icons/icon-header-01.png', alt='ICON')
            span.linedivide2
            a.header-wrapicon2(href='/cart')
              img.header-icon1.js-show-header-dropdown(src='images/icons/icon-header-02.png', alt='ICON')
              span#quantity.header-icons-noti 0
          .btn-show-menu-mobile.hamburger.hamburger--squeeze
            span.hamburger-box
              span.hamburger-inner
      // Menu Mobile
      .wrap-side-menu
        nav.side-menu
          ul.main-menu
            li.item-topbar-mobile.p-l-20.p-t-8.p-b-8
              span.topbar-child1
                | Free shipping for standard order over $100
            li.item-topbar-mobile.p-l-20.p-t-8.p-b-8
              .topbar-child2-mobile
                span.topbar-email
                  | info@tingfashion.com
                .topbar-language.rs1-select2
                  select.selection-1(name='time')
                    option USD
                    option EUR
            li.item-topbar-mobile.p-l-10
              .topbar-social-mobile
                a.topbar-social-item.fa.fa-facebook(href='#')
                a.topbar-social-item.fa.fa-instagram(href='#')
                a.topbar-social-item.fa.fa-pinterest-p(href='#')
                a.topbar-social-item.fa.fa-snapchat-ghost(href='#')
                a.topbar-social-item.fa.fa-youtube-play(href='#')
            li.item-menu-mobile
              a(href='/') Home
            li.item-menu-mobile
              a(href='/products') Shop
            li.item-menu-mobile
              a(href='/products') Sale
            li.item-menu-mobile
              a(href='/cart') Features
            li.item-menu-mobile
              a(href='/blog') Blog
            li.item-menu-mobile
              a(href='/about') About
            li.item-menu-mobile
              a(href='/contact') Contact
    
